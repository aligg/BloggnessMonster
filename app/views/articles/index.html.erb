

<% @articles.each do |article| %>
  <dl>
    <dt><h1><%= article.title %></h1></dt>
    <dd><%= article.text %></dd>
    <dd><%= link_to 'Comment', article_path(article) %></dd>
    <dd><%= link_to 'Edit', edit_article_path(article) %></dd>
    <dd><%= link_to 'Delete', article_path(article),
                    method: :delete, data: { confirm: 'Are you sure?' } %></dd>
  </dl>
<% end %>



</br>
